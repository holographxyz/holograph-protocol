{
  "name": "holograph-token-creator",
  "version": "1.0.0",
  "description": "TypeScript utility for deploying ERC20 tokens via Holograph protocol",
  "main": "dist/script/create-token.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch script/create-token.ts",
    "start": "node dist/script/ts/create-token.js",
    "create-token": "tsx script/ts/create-token.ts",
    "multisig-cli": "tsx script/ts/multisig-cli.ts",
    "multisig-cli:help": "tsx script/ts/multisig-cli.ts help",
    "multisig-cli:batch": "tsx script/ts/multisig-cli.ts batch",
    "multisig-cli:deposit": "tsx script/ts/multisig-cli.ts deposit",
    "multisig-cli:transfer-ownership": "tsx script/ts/multisig-cli.ts transfer-ownership",
    "multisig-cli:accept-ownership": "tsx script/ts/multisig-cli.ts accept-ownership",
    "multisig-cli:pause": "tsx script/ts/multisig-cli.ts pause",
    "multisig-cli:unpause": "tsx script/ts/multisig-cli.ts unpause",
    "type-check": "tsc --noEmit",
    "lint": "eslint script/*.ts",
    "lint:fix": "eslint script/*.ts --fix",
    "format": "prettier --write script/*.ts *.json *.md",
    "format:check": "prettier --check script/*.ts *.json *.md",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build"
  },
  "dependencies": {
    "@safe-global/protocol-kit": "^6.1.0",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.0.0",
    "viem": "^2.8.18"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "holograph",
    "doppler",
    "token",
    "erc20",
    "base",
    "uniswap",
    "defi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holograph/token-creator.git"
  },
  "license": "MIT"
}
